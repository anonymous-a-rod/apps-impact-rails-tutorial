<h1>List Teachers</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Bio</th>
    </tr>
  </thead>
  <tbody>
  <% @teachers.each do |teacher| %>
    <tr>
      <td><%= teacher.first_name %></td>
      <td><%= teacher.last_name %></td>
      <td><%= teacher.biography %></td>
    </tr>
  <% end %>
  </tbody>
</table>